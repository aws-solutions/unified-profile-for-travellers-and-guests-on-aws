        "redux-persist": "^6.0.0",
        "sinon": "^17.0.1",
        "uuid": "^9.0.1",
        "web-vitals": "^3.1.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.4.6",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "^29.5.12",
        "@types/luxon": "^3.4.1",
        "@types/mocha": "^10.0.6",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@types/sinon": "^17.0.3",
        "@types/uuid": "^9.0.7",
